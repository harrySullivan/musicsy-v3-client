{
  "name": "server",
  "version": "1.0.0",
  "description": "It's like spotify premium with all the premium features",
  "main": "index.js",
  "directories": {
    "test": "tests"
  },
  "scripts": {
    "dev-server": "nodemon --exec go run App ./App.go --ignore client/",
    "bust-cache": "node utils/cache.js",
    "dev-client": "npx parcel client/index.html",
    "build-client": "npx rimraf dist && npx parcel build client/index.html --public-url /static/ && npm run bust-cache",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/harrySullivan/musicsy-v3.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/harrySullivan/musicsy-v3/issues"
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "^5.11.2",
    "@types/node": "^12.7.4",
    "bulma": "^0.7.5",
    "cheerio": "^1.0.0-rc.3",
    "parcel-bundler": "^1.12.3",
    "preact": "^8.5.2",
    "preact-router": "^2.6.1",
    "rimraf": "^3.0.0",
    "spacing": "^0.3.0",
    "typescript": "^3.6.2",
    "unistore": "^3.4.1"
  },
  "alias": {
    "$root": "./client"
  },
  "homepage": "https://github.com/harrySullivan/musicsy-v3#readme"
}
